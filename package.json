{
  "name": "neaten",
  "version": "1.0.0",
  "description": "TypeScript-friendly dependency injection for JavaScript",
  "main": "built/src/index.js",
  "typings": "built/src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "tape built/tests/**/*Tests.js",
    "coverage": "istanbul cover tape built/test/**/*Tests.js",
    "all": "npm run build && npm run test && npm run coverage",
    "prepublish": "npm run build && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielearwicker/neaten.git"
  },
  "keywords": [
    "Dependency Injection",
    "TypeScript"
  ],
  "author": "Daniel Earwicker <dan@earwicker.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danielearwicker/neaten/issues"
  },
  "homepage": "https://github.com/danielearwicker/neaten#readme",
  "dependencies": {
    "coveralls": "^3.0.2",
    "faucet": "0.0.1",
    "immutable": "^3.8.2",
    "istanbul": "^0.4.5"
  },
  "devDependencies": {
    "@types/blue-tape": "^0.1.31",
    "blue-tape": "^1.0.0",
    "tslint": "^5.10.0",
    "typescript": "^3.0.1"
  }
}
